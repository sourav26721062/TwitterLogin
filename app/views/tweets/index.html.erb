
<table class="table table-striped">
<tr>
<th>Tweet UrL</th>
<th>Tweet</th>
<th>Date</th>
</tr>
<% @url_tweet.each do |t| %>
<tr>
<td><%= t.url %></td>
<td><%= t.tweet %></td>
<td><%= t.date%></td>
</tr>
<% end %>
</table>
<br/>
<center>
<p>Max tweets by <%= @max_user %> Tweets : <%= @max %></p></center>